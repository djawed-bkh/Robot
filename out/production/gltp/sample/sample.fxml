                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
            <Button fx:id="listeenseignant" layoutX="105.0" layoutY="151.0" mnemonicParsing="false" onAction="#afficherenseignant" prefHeight="53.0" prefWidth="226.0" text="Afficher enseignant ">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
            <Label layoutX="274.0" layoutY="56.0" prefHeight="44.0" prefWidth="180.0" text="            Menu">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="ButtonOuvrages" layoutX="106.0" layoutY="327.0" mnemonicParsing="false" onAction="#afficherOuvrages" prefHeight="53.0" prefWidth="226.0" text="Afficher ouvrages demandées">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
            <Button fx:id="ButtonListeNoir" layoutX="379.0" layoutY="327.0" mnemonicParsing="false" onAction="#AfficherListeNoir" prefHeight="53.0" prefWidth="226.0" text="Afficher liste noir">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
