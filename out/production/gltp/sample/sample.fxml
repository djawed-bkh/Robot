<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="sample" prefHeight="720.0" prefWidth="816.0">
         <children>
            <MenuBar prefHeight="37.0" prefWidth="836.0">
              <menus>
                <Menu mnemonicParsing="false" text="Parametres">
                  <items>
                    <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#closeMet" text="Quitter" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Button fx:id="listeetudiant" layoutX="105.0" layoutY="236.0" mnemonicParsing="false" onAction="#afficheretudiants" prefHeight="53.0" prefWidth="226.0" text="Afficher etudiant">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
            <Button fx:id="liste" layoutX="379.0" layoutY="236.0" mnemonicParsing="false" onAction="#afficherDoctorants" prefHeight="53.0" prefWidth="226.0" text="Afficher doctorants">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Button>
            <Button fx:id="listemploye" layoutX="379.0" layoutY="151.0" mnemonicParsing="false" onAction="#afficheremploye" prefHeight="53.0" prefWidth="226.0" text="Afficher employé">
